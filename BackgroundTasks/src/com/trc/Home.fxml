<AnchorPane prefHeight="263.0" prefWidth="323.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="com.trc.HomeController">
   <children>
      <VBox layoutX="49.0" layoutY="32.0" prefHeight="103.0" prefWidth="227.0">
         <children>
            <ProgressBar fx:id="progressCounter" prefHeight="27.0" prefWidth="231.0" progress="0.0" />
            <ProgressBar fx:id="progressCounter1" prefHeight="27.0" prefWidth="227.0" progress="0.0" />
         </children>
      </VBox>
      <Button fx:id="btnStartProgress" layoutX="92.0" layoutY="132.0" mnemonicParsing="false" onAction="#startCountingAndUpdateProgressBar" text="Start Counting" />
      <Button fx:id="btnStopProgress" layoutX="92.0" layoutY="192.0" mnemonicParsing="false" onAction="#stopCountingAndResetProgressBar" text="Stop Counting" />
   </children>
</AnchorPane>
